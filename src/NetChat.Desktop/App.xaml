                <ResourceDictionary Source="./View/Styles/ScrollBarStyle.xaml"/>
                <ResourceDictionary Source="./View/Styles/ButtonStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>

            
        </ResourceDictionary>
